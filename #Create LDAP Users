#Create LDAP Users


#El usuario lnavarror pertenece al depInformatica
dn: uid=lnavarror,ou=usuarios,ou=depInformatica,dc=altamira,dc=raiz
objectClass: inetOrgPerson
objectClass: posixAccount
objectClass: shadowAccount
uid: lnavarror
sn: navarro
givenName: luis
cn: luis navarro
displayName: lnavarror Informática
uidNumber: 10000
gidNumber: 5000
userPassword: 
loginShell: /bin/bash
homeDirectory: /home/lnavarror

